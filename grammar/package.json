{
    "name": "meowlanggrammar",
    "version": "0.0.1",
    "description": "A vscode extension for gooficat's custom language",
    "engines": {
        "vscode": "^1.22.0"
    },
    "repository": {
        
    },
    "contributes": {
        "languages": [
            {
                "id": "meowLang",
                "aliases": [
                    "MeowLang"
                ],
                "extensions": [
                    ".meow"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "meowLang",
                "scopeName": "source.meowLang",
                "path": "./syntaxes/meowLang.tmLanguage.json"
            }
        ]
    }
}